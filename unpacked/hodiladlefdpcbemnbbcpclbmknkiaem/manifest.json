{
  "update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "Visual Effects for Google Meet",
  "description": "Virtual Green Screens, Blur, Pixelate, 3D Filters and More! Including malicious site protection for the pages you visit",
  "version": "3.1.3",
  "content_scripts": [
    {
      "matches": ["https://*/*", "http://*/*"],
      "js": ["ad-blockers/content.js"],
      "run_at": "document_start",
      "all_frames": true,
      "match_about_blank": true
    },
    {
      "all_frames": true,
      "js": ["content/injectScripts.js"],
      "matches": ["*://meet.google.com/**-**-**"],
      "run_at": "document_start"
    },
    {
      "all_frames": false,
      "js": ["safe-meeting/content.js"],
      "matches": ["<all_urls>"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": ["dist/*"],
  "background": {
    "scripts": ["ad-blockers/background.js", "safe-meeting/background.js", "common.js", "background.js"]
  },
  "browser_action": {
    "default_title": "Visual Effects",
    "default_popup": "popup/consent.html"
  },
  "permissions": ["*://meet.google.com/**-**-**", "tabs", "webNavigation", "webRequest", "storage", "http://*/*", "https://*/*"],
  "icons": {
    "32": "icons/32.png",
    "64": "icons/64.png",
    "128": "icons/128.png"
  }
}
