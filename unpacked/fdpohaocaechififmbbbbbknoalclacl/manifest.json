{
  "update_url": "https://clients2.google.com/service/update2/crx",

  "name": "GoFullPage - Full Page Screen Capture",
  "version": "7.12",
  "manifest_version": 2,
  "default_locale": "en",
  "description": "__MSG_appDesc__",
  "browser_action": {
    "default_icon": "images/icon-999.png",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+Shift+P"
      }
    }
  },
  "background": {
    "scripts": ["js/background/index.js"],
    "persistent": false
  },
  "minimum_chrome_version": "49",
  "permissions": ["activeTab", "storage", "unlimitedStorage"],
  "optional_permissions": ["downloads", "<all_urls>", "webNavigation", "file://*/*"],
  "web_accessible_resources": ["p/*"],
  "icons": {
    "16": "images/icon16-999.png",
    "48": "images/icon48-999.png",
    "128": "images/icon128-999.png"
  }
}
